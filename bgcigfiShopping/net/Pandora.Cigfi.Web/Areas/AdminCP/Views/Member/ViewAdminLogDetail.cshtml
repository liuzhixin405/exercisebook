@model  Sys_AdminLogModel
@{ 
    ViewBag.title = "查看管理日志详情";
}

<div class="wrapper wrapper-content ">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>@ViewBag.title</h5>
            <div class="ibox-tools">
            </div>
        </div>
        <div class="ibox-content">
            @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "editGuestbookForm", @class = "form-horizontal m-t" }))
    {
        <div class="form-group">
            <label class="col-sm-3 control-label">唯一编号：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.GUID</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">用户帐号：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.UserName</label>
            </div>
        </div>

       
        <div class="form-group">
            <label class="col-sm-3 control-label">操作时间：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.LogTime.ToString("yyyy-MM-dd HH:mm")</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">操作IP：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.LogIP</label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">日志内容：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.LogMsg </label>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">操作类型：</label>
            <div class="col-sm-8">
                <label class="control-label">@Model.LogType</label>
            </div>
        </div>

        <div class="hr-line-dashed"></div>
        <div class="form-group">
            <div class="col-sm-4 col-sm-offset-3">
                <button class="btn btn-default" type="button" onclick="closethisdialog();"><i class="fa fa-close"></i> 关闭 </button>
            </div>
        </div>
}
        </div>
    </div>


</div>