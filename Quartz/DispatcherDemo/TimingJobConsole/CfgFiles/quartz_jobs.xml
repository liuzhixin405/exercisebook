			<durable>true</durable>
			<recover>false</recover>
		</job>
		<trigger>
			<cron>
				<name>GoodJobTrigger1</name>
				<group>group1</group>
				<job-name>GoodJob</job-name>
				<job-group>group1</job-group>
				<cron-expression>5/10 * * * * ?</cron-expression>
			</cron>
		</trigger>


