  services.AddGrpcServer();
            services.AddGrpcClient(options =>
            {
                Configuration.GetSection("gRPC").Bind(options);
            });services.RegisterAssembly("Mjb.Erp2.Ordering.IService", "Mjb.Erp2.Ordering.ServiceImpl", ServiceLifetime.Scoped, interceptedClasses, interfacTypeCallback: type =>
            {
                if (type.Name.StartsWith("IComOrderNoFactory"))
                {
                    return true;
                }

                return false;
            });
