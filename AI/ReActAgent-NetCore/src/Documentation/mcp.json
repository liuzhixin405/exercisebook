{
  "mcpServers": {
    "filesystem": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
         "G:\\github\\python_exercise\\musiccreation"
      ],
      "autoApprove": [
        "read_file",
        "read_multiple_files",
        "write_file",
        "edit_file",
        "create_directory",
        "list_directory",
        "move_file",
        "directory_tree",
        "search_files",
        "get_file_info"
      ],
      "disabled": false,
      "alwaysAllow": [
        "read_file",
        "read_multiple_files",
        "write_file",
        "edit_file",
        "create_directory",
        "list_directory",
        "move_file",
        "search_files",
        "directory_tree",
        "get_file_info",
        "list_allowed_directories"
      ]
    },
    "fetch-server": {
      "command": "python",
      "args": [
        "-m",
        "mcp_server_fetch"
      ]
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "alwaysAllow": [
        "sequentialthinking"
      ]
    },
    "context7": {
      "command": "cmd",
      "args": [
        "/c", 
        "npx", 
        "-y", 
        "@upstash/context7-mcp@latest"
      ]
    },
    "mysql": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "mysql-mcp-server",
        "mysql_mcp_server"
      ],
      "env": {
        "MYSQL_HOST": "127.0.0.1",
        "MYSQL_PORT": "3307",
        "MYSQL_USER": "root",
        "MYSQL_PASSWORD": "123456",
        "MYSQL_DATABASE": "musiccreation"
      }
    },
    "project-creator": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-project-creator"
      ],
      "alwaysAllow": [
        "create_project",
        "list_templates",
        "get_project_info"
      ]
    },
    "code-analyzer": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-code-analyzer"
      ],
      "alwaysAllow": [
        "analyze_code",
        "get_code_metrics",
        "suggest_improvements"
      ]
    },
    "git-helper": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-git"
      ],
      "alwaysAllow": [
        "git_status",
        "git_commit",
        "git_push",
        "git_pull",
        "git_branch"
      ]
    }
  }
}